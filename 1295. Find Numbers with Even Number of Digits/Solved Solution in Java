class Solution {
    public int findNumbers(int[] nums) {
        int count = 0;
        for (int i = 0 ;i< nums.length ;i++){
            int digit_count = (int)Math.floor(Math.log10(nums[i]) + 1);
            if((digit_count & 1) != 1){
                count++;
            }
        }
        return count;
    }
}
